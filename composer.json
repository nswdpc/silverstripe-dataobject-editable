{
    "name": "nswdpc/silverstripe-module-skeleton",
    "description": "The NSWDPC skeleton for Silverstripe modules.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "nsw",
        "module",
        "skeleton",
        "more keywords"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Name",
            "email": "email@example.com",
            "homepage": "https://dpc.nsw.gov.au",
            "role": "Optional role"
        }
    ],
    "support": {
        "key" : "value"
    },
    "require": {
        "package" : "^version"
    },
    "replace" : {
        "package" : "version"
    },
    "suggest" : {
        "package" : "reason"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    }
}
